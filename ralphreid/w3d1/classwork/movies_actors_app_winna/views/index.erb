<h3>Actors Index</h3>
<ul>
<% @actors.each do |actor| %>
  <li>
    <h4>
      <a href="/actors/<%= actor["id"]%>">
        <%= "#{actor["first_name"]} #{actor["last_name"]}"%>
      </a>
    </h4>
  </li>
<% end %>
</ul>

<h3>Movie Index</h3>

<ul>
<% @movies.each do |movie| %>
  <li>
    <h4>
      <a href="/movies/<%= movie["id"]%>">
        <%= "#{movie["title"]}"%>
      </a>
    </h4>
  </li>
<% end %>
</ul>