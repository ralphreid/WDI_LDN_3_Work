<h3> <%= @actor["first_name"] %> <%= @actor["last_name"] %>  </h3>

<img src="<%= @actor["image_url"] %>" />

<p>First Name: <%= @actor["first_name"] %></p>
<p>Last Name: <%= @actor["last_name"] %></p>
<p>Date of Birth: <%= @actor["dob"] %></p>

<form method="POST" action="/actors/<%= @actor["id"] %>/delete">
  <input type="submit" value="delete" /> 
</form>
<a href="/actors/<%= @actor["id"] %>/update">Update this Actor</a>